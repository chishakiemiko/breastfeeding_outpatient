          <div class="col-lg-9 p-5  border border-danger">
            <h2 class="paitiant_list_title">乳房状態</h2>
 

            <%#= form_with model: [patient, patient.breast_conditions.new], local: true, method: :post do |form| %>
            <%= form_with model: [@patient, @breast_condition], local: true, method: :post do |form| %>
              <div class ="row">
                <div class="text_field mb-2">
                  <%= form.label :chief_complaint, "主訴" %>
                  <%= form.text_area :chief_complaint, class: 'form-control', size: "60x5" %>
                <div class="text_field mb-2">
                  <%= form.label :progress_of_visit, "来院までの経過" %>
                  <%= form.text_area :progress_of_visit ,class: 'form-control', size:"60x10" %>
                </div>
                <hr>
                <h5>乳房実質について</h5>
                <hr>
                  <div class="form-group mb-4">
                  <%= form.label :breast_type, "乳房タイプ" ,class: 'me-2'%><br>
                  <%= form.label :breast_type, "Ⅰ", {value: "Ⅰ", style: "display: inline-block;"} %>
                  <%= form.radio_button :breast_type, "Ⅰ" ,class: 'me-2' %>
                  <%= form.label :breast_type, "Ⅱa", {value: "Ⅱa", style: "display: inline-block;"} %>
                  <%= form.radio_button :breast_type, "Ⅱa" %>
                  <%= form.label :breast_type, "Ⅱb", {value: "Ⅱb", style: "display: inline-block;"} %>
                  <%= form.radio_button :breast_type, "Ⅱb" ,class: 'me-2' %>
                  <%= form.label :breast_type, "Ⅲ", {value: "Ⅲ", style: "display: inline-block;"} %>
                  <%= form.radio_button :breast_type, "Ⅲ" %>
                </div>
                <hr>
                <h6>温かさ</h6>
                  <div class="d-flex justify-content-around">
                    <div class="form-group mb-4">
                      <%= form.label :udder_warmth_right, "(右)" ,class: 'me-2'%>
                      <%= form.label :udder_warmth_right, "++", {value: "++", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_right, "++" ,class: 'me-2' %>
                      <%= form.label :udder_warmth_righ, "+", {value: "+", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_right, "+" %>
                      <%= form.label :udder_warmth_righ, "±", {value: "±", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_right, "±" ,class: 'me-2' %>
                      <%= form.label :udder_warmth_righ, "-", {value: "-", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_right, "-" %>
                    </div>
                
                    <div class="form-group mb-4">
                      <%= form.label :udder_warmth_left, "(左)" ,class: 'me-2'%>
                      <%= form.label :udder_warmth_left, "++", {value: "++", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_left, "++" ,class: 'me-2' %>
                      <%= form.label :udder_warmth_left, "+", {value: "+", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_left, "+" %>
                      <%= form.label :udder_warmth_left, "±", {value: "±", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_left, "±" ,class: 'me-2' %>
                      <%= form.label :udder_warmth_left, "-", {value: "-", style: "display: inline-block;"} %>
                      <%= form.radio_button :udder_warmth_left, "-" %>
                    </div>
                  </div>
                    
                <hr>
