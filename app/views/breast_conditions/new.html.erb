                    <%= form.radio_button :udder_tension_right, "±" ,class: 'me-2' %>
                    <%= form.label :udder_tension_right, "-", {value: "-", style: "display: inline-block;"} %>
                    <%= form.radio_button :udder_tension_right, "-" %>
                  </div>
                  <div class="form-group mb-4">
                    <%= form.label :udder_tension_left, "(左)" ,class: 'me-2'%>
                    <%= form.label :udder_tension_left, "++", {value: "++", style: "display: inline-block;"} %>
                    <%= form.radio_button :udder_tension_left, "++" ,class: 'me-2' %>
                    <%= form.label :udder_tension_left, "+", {value: "+", style: "display: inline-block;"} %>
                    <%= form.radio_button :udder_tension_left, "+" %>
                    <%= form.label :udder_tension_left, "±", {value: "±", style: "display: inline-block;"} %>
                    <%= form.radio_button :udder_tension_left, "±" ,class: 'me-2' %>
                    <%= form.label :udder_tension_left, "-", {value: "-", style: "display: inline-block;"} %>
                    <%= form.radio_button :udder_tension_left, "-" %>
                  </div>
                </div>
                
                <hr>
                <h6>副乳</h6>
