                    <%= form.radio_button :breast_size_difference, "R >= L" ,class: 'me-2'%>
                    <%= form.label :breast_size_difference, "R > L", {value: "R > L", style: "display: inline-block;"} %>
                    <%= form.radio_button :breast_size_difference, "R > L" %>
