                    <div class="form-group mb-4">
                      <%= form.label :sub_breast_right, "(右)" ,class: 'me-2'%>
                      <%= form.label :sub_breast_right, "あり", {value: "あり", style: "display: inline-block;"} %>
                      <%= form.radio_button :sub_breast_right, "あり" ,class: 'me-2' %>
                      <%= form.label :sub_breast_right, "なし", {value: "なし", style: "display: inline-block;"} %>
                      <%= form.radio_button :sub_breast_right, "なし" %>
