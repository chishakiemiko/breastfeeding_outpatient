<% if @patient.errors.full_messages.present? %>
  <div class=" py-3 bg-danger text-center text-white fs-5">
    <ul class="form__error priority">
      <% @patient.errors.full_messages.each do |msg| %>
        <li class="form__error__item priority">
            <%= msg %>
        </li>
      <% end %>
    </ul>
  </div>
<% end %>


